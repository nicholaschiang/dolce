{
  "name": "nicholas-eng",
  "version": "1.0.1",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s 'build:*'",
    "build:css": "yarn generate:css --minify",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle",
    "dev": "run-p 'dev:*' | pino-pretty",
    "dev:build": "cross-env NODE_ENV=development yarn build:server --watch",
    "dev:css": "cross-env NODE_ENV=development yarn generate:css --watch",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -m -i ./app/app.css --postcss ./postcss.config.js -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint --ext .js,.ts,.jsx,.tsx app",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js | pino-pretty",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "yarn build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "run-p 'typecheck:*'",
    "typecheck:app": "tsc -b",
    "typecheck:scraper": "tsc -b scraper",
    "typecheck:cypress": "tsc -b cypress",
    "validate": "run-p \"test --run\" lint typecheck test:e2e:run",
    "scrape": "run-s 'scrape:*'",
    "scrape:build": "tsc -p scraper/node",
    "scrape:run": "node scraper/node/out/aritzia.js | pino-pretty"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@radix-ui/react-checkbox": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-popover": "^1.0.5",
    "@radix-ui/react-portal": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.5",
    "@remix-run/express": "1.11.1",
    "@remix-run/node": "1.11.1",
    "@remix-run/react": "1.11.1",
    "@remix-run/server-runtime": "1.11.1",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.2",
    "cmdk": "^0.2.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dequal": "^2.0.3",
    "express": "^4.18.2",
    "express-prometheus-middleware": "^1.2.0",
    "isbot": "^3.6.7",
    "morgan": "^1.10.0",
    "nanoid": "^4.0.1",
    "pino": "^8.11.0",
    "prom-client": "^14.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^4.3.8",
    "rfdc": "^1.3.0",
    "sharp": "^0.32.0",
    "tailwindcss-radix": "^2.8.0",
    "tiny-invariant": "^1.3.1",
    "ts-key-enum": "^2.0.12"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
