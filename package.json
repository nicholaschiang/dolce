    "autoprefixer": "^10.4.13",
    "c8": "^7.12.0",
    "cheerio": "^1.0.0-rc.12",
    "cookie": "^0.5.0",
    "cypress": "^12.4.1",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.5",
    "eslint": "^8.33.0",
