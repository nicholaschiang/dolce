{
  "name": "nicholas-eng",
  "version": "1.5.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "run-s 'build:*'",
    "build:prisma": "prisma generate",
    "build:css": "pnpm generate:css --minify",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle",
    "vercel-build": "replace 'prisma/client' 'prisma/client/edge' app -r && pnpm build",
    "dev": "run-p 'dev:*' | pino-pretty",
    "dev:prisma": "prisma generate --watch",
    "dev:build": "cross-env NODE_ENV=development pnpm build:server --watch",
    "dev:css": "cross-env NODE_ENV=development pnpm generate:css --watch",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -m -i ./app/app.css --postcss ./postcss.config.js -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint --ext .js,.ts,.jsx,.tsx app",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js | pino-pretty",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"pnpm cypress open\"",
    "pretest:e2e:run": "pnpm build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"pnpm cypress run\"",
    "typecheck": "run-p 'typecheck:*'",
    "typecheck:app": "tsc -b",
    "typecheck:scraper": "tsc -b scraper/node",
    "typecheck:cypress": "tsc -b cypress",
    "validate": "run-p \"test --run\" lint typecheck test:e2e:run",
    "scrape": "run-s 'scrape:*'",
    "scrape:build": "tsc -p scraper/node",
    "scrape:run": "node scraper/node/out/aritzia.js | pino-pretty"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
